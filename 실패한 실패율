def solution(N, stages):
    answer = []
    people = len(stages)
    for i in range(1,N+1):
        if people > 0:
            fail = 0
            fail = stages.count(i)/people
        else:
            fail = 0
        people -= stages.count(i)
        answer.append(str(1-fail) + str(i))
    answer.sort()
    for j in range(len(answer)):
        answer[j] = int(answer[j][-1:])
    return answer
