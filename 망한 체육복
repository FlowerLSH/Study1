def solution(n, lost, reserve):
    for i in lost:
        if i in reserve:
            reserve.remove(i)
            lost.remove(i)
    for i in reserve:
        if len(reserve) < 1 and len(lost) < 1:
            break
        else:
            for j in lost:
                if (j - 1 == i or j + 1 == i):
                    reserve.remove(i)
                    lost.remove(j)
    answer = n - len(lost)
    return answer
